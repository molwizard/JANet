              <option value="select_gene">Ageing Factor - Gene</option>
              <option value="select_compound">Ageing Factor - Compound</option>
              <option value="select_otherfactor">Ageing Factor - Other Factor</option>
              <option value="select_observation">Observation</option>
              <option value="select_observationlifespan">Lifespan Observation</option>
              <option value="select_observationincreased">Lifespan Observation - increased</option>
              <option value="select_observationdecreased">Lifespan Observation - decreased</option>
              <option value="select_observationnone">Lifespan Observation - unchanged</option>
              <option value="select_observationhomology">Homology Analysis Observation</option>
              <option value="select_species">Species</option>
              <option value="select_alleletype">Allele type</option>
              <option value="select_citation">Citation</option>
              <option value="select_goterm">GO term</option>
              <option value="select_gotermprocess">GO Process Term</option>
              <option value="select_gotermfunction">GO Function Term</option>
              <option value="select_gotermcompartment">GO Compartment Term</option>
              <option value="select_keggpw">KEGG Pathway</option>
              <option value="select_keggpwgene">KEGG Pathway Gene</option>
          </select>
        &nbsp;<input class="jmol advanced_inline"id="select_node_type_only2" value="only" type="button"><span id="select_node_type_controls_adv" class="advanced_inline"><input class="jmol" id="select_node_type_add" value="&nbsp;+&nbsp;" type="button"><input class="jmol" id="select_node_type_subtract" value="&nbsp;-&nbsp;" type="button"><input class="jmol" id="select_node_type_and" value="&nbsp;&amp;&nbsp;" type="button"><input class="jmol" id="select_node_type_not" value="not" type="button"></span>
          </div>

          <div class="jsnet3d">
            <label class="jsnet3d selectGroup">Species:</label>
            <select class="jmol" id="select_species_menu2">
              <option value="select_A_gambiae">Anopheles gambiae</option>
              <option value="select_A_thaliana">Arabidopsis thaliana</option>
